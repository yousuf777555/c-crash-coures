#include<stdio.h>
#define N 10
int main()
{
    int a[N]={1,2,3,4,5},b;
    int i,c=0;
    printf("enter the element to find");
    scanf("%d",&b);
    for(i=0;i<N;i++)
    {
    if((a[i] ^ b) == 0)
        {
            c++;
            break;
        }
    }
    if(c>0)
    {
        printf("element present");
    }
    else if(c==0)
    {
        printf("element not present");
    }
    
}
