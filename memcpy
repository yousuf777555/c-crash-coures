#include<stdio.h>
#include<string.h>
void mymemcpy(void *des,const void src,unsigned int n);
int main()
{
    char src[50] = "mohd yousuf";
    char des[50];
    char *dest = mymemcpy(des,src,strlen(src)+1);
    printf("%s\n",dest);
    
}

void * mymemcpy(void *des,void const *src,unsigned int n)
{
    int i =0;
    char *dest = (char *)des;
    char *srce = (char *)src;
    for(i=0;i<n;i++)
    {
        *(dest+i) = *(srce+i);
    }
    
   return des;
}
